{
  "name": "electronicsheet",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:light": "cross-env NEXT_TELEMETRY_DISABLED=1 next dev --no-lint --no-type-check --no-version-check",
    "build": "npm run build:check-db && next build",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "prisma migrate dev",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "build:check-db": "node -e \"if (process.env.DATABASE_URL && !process.env.DATABASE_URL.includes('file:./dev.db')) { require('child_process').execSync('prisma generate', { stdio: 'inherit' }) } else { console.log('Skipping Prisma generate - no database configured') }\"",
    "postinstall": "npm run build:check-db"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@prisma/client": "^6.19.0",
    "@tinymce/tinymce-react": "^6.3.0",
    "next": "16.0.6",
    "prisma": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sweetalert2": "^11.26.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
